	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="myBatisSelectList">
	<resultMap type="seed" id="resultMap">
		<result property="site" column="SITE_ID" javaType="java.lang.String"/>
		<result property="seed" column="SEED_ID" javaType="java.lang.String"/>
		<result property="type" column="SEED_TYPE" javaType="java.lang.String"/>
		<result property="crawlCount" column="VISIT_CNT" javaType="long"/>
		<result property="crawledDocCount" column="DOC_CNT" javaType="long"/>
		<result property="lastCrawledDate" column="LAST_VISIT_DATE" javaType="long"/>
		<result property="firstCrawledDate" column="FIRST_VISIT_DATE" javaType="long"/>
		<result property="cursor" column="CURSOR" javaType="java.lang.String"/>
	</resultMap>
	<select id="selectSeedInfo" resultMap="resultMap">
		SELECT *
		FROM seed_info
	</select>
